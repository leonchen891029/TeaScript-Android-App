    <style name="team_member_information" parent="@style/wrap_wrap">
        <item name="android:padding">5dp</item>
        <item name="android:textColor">?attr/android:textColor</item>
    </style>

    <style name="team_new_issue_layout" parent="@style/match_wrap">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">?attr/layout_item_bg</item>
