<resources>

    <style name="list_cell_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">#cccccc</item>
    </style>

    <style name="text_base" parent="@style/wrap_wrap">
        <item name="android:textSize">@dimen/text_regular_primary_size</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="text_multiline"  parent="@style/text_base">
        <item name="android:maxLines">5</item>
        <item name="android:singleLine">false</item>
        <item name="android:lineSpacingMultiplier">1.5</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!--导航栏颜色 -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!--控件被选中时的颜色-->
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.AppStartLoad" parent="android:Theme.Black.NoTitleBar.Fullscreen" >
        <item name="android:windowBackground">@drawable/welcome</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="PreviewPhotos" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

    <!--button-->
    <style name="button_common_blue">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:background">@drawable/btn_blue_selector</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_16</item>
    </style>

    <style name="fullline_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--Dialog Styles-->
    <style name="dialog_pinterest_text" parent="@style/text_multiline">
        <item name="android:textSize">@dimen/text_regular_primary_size</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="dialog_title" parent="@style/fullline_view">
        <item name="android:textSize">@dimen/text_large_primary_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="dialog_subtitle" parent="@style/fullline_view">
        <item name="android:textSize">@dimen/text_large_secondary_size</item>
        <item name="android:textColor">@color/text_light</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">2</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    <style name="dialog_divider">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="dialog_event_apply_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/space_2</item>
        <item name="android:textColor">@color/main_gray</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:layout_marginTop">@dimen/space_10</item>
    </style>

    <style name="dialog_event_apply_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/comment_edittext_selector</item>
        <item name="android:gravity">top</item>
        <item name="android:textColor">@color/main_black</item>
        <item name="android:textColorHint">@color/main_gray</item>
        <item name="android:padding">@dimen/space_5</item>
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>

    <style name="dialog_common" parent="@style/Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@drawable/dialog_background</item>
    </style>

    <style name="dialog_button">
        <item name="android:layout_width">0.0dp</item>
        <item name="android:layout_height">46.0dp</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:maxLines">2</item>
        <item name="android:textSize">@dimen/text_regular_primary_size</item>
        <item name="android:textColor">@color/text_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/touch_clear_bg_selector</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="quick_option_dialog" parent="@style/dialog_common">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowAnimationStyle">@style/dialog_animation</item>
    </style>

    <style name="dialog_bottom" parent="@style/dialog_common">
        <item name="android:windowBackground">@drawable/dialog_bottom_background</item>
        <item name="android:windowAnimationStyle">@style/dialog_animation</item>
    </style>

    <style name="dialog_animation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>

    <style name="section_item_divider">
        <item name="android:background">@color/ui_dialog_divider</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>


    <style name="section_item_divider_vertical">
        <item name="android:background">@color/ui_dialog_divider</item>
        <item name="android:layout_width">0.1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!--toolbar-->
    <style name="toolbar_action">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">center</item>
        <item name="android:minWidth">44.0dip</item>
        <item name="android:layout_weight">0.0</item>
    </style>

    <style name="toolbar_action_comment">
        <item name="android:textSize">8.0sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/main_green</item>
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingRight">2.0dip</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginTop">-8.0dip</item>
        <item name="android:maxWidth">44.0dip</item>
        <item name="android:minWidth">12.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="user_face_image">
        <item name="android:layout_width">28.0dip</item>
        <item name="android:layout_height">28.0dip</item>
        <item name="android:padding">1.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:src">@drawable/widget_dface</item>
    </style>

    <style name="list_text_info" parent="@style/wrap_wrap">
        <item name="android:textColor">?attr/infoTextColor</item>
        <item name="android:textSize">11sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="list_cell_text">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="list_cell_text_dialog" parent="@style/list_cell_text">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/ui_dialog_list_text</item>
    </style>

    <style name="h_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?attr/lineColor</item>
    </style>

    <!-- 比较常用的宽高 -->
    <style name="wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wrap_match">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="list_text_title" parent="@style/wrap_wrap">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--team-->
    <style name="team_issue_detail_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">18dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center|left</item>
    </style>

    <style name="team_issue_detail_info_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="team_issue_detail_tip_fa" parent="@style/wrap_wrap">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">@color/main_gray</item>
    </style>

    <style name="team_issue_detail_tip" parent="@style/wrap_wrap">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="team_issue_detail_text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">right</item>
        <item name="android:textColor">?attr/infoTextColor</item>
    </style>

    <style name="team_issue_detail_line" parent="@style/h_line">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="myissue_text" parent="@style/match_wrap">
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="myissue_text_num" parent="@style/myissue_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:text">0</item>
    </style>

    <style name="comment_user_face_image">
        <item name="android:layout_width">28.0dip</item>
        <item name="android:layout_height">28.0dip</item>
        <item name="android:padding">1.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:src">@drawable/widget_dface</item>
    </style>


    <!-- events title -->
    <style name="events_list_title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">@dimen/text_size_12</item>
    </style>

    <style name="events_list_content">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dip</item>
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>

    <style name="events_list_commits">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dip</item>
        <item name="android:textColor">#999999</item>
        <item name="android:tag">1</item>
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>

    <style name="events_list_client">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10dip</item>
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">@dimen/text_size_10</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="events_list_data">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">@dimen/text_size_10</item>
    </style>

    <!-- viewpage文本  滑动标签标题 -->
    <style name="viewpage_slidingTabTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/viewpager_slide_title_selector</item>
        <item name="android:textSize">17sp</item>
    </style>

    <!--操作项样式-->
    <style name="option_item_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/icon_material</item>
        <item name="android:clickable">true</item>
        <item name="android:padding">@dimen/space_12</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="option_item_text_parent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/space_4</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/main_black</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">@dimen/text_size_16</item>
    </style>


    <style name="option_item_text" parent="@style/option_item_text_parent">
        <item name="android:drawablePadding">@dimen/space_8</item>
        <item name="android:textColor">?attr/android:textColor</item>
    </style>

    <style name="option_item_text_fa" parent="@style/option_item_text_parent">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">?android:textColorLink</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="option_item_img">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_marginRight">@dimen/space_10</item>
    </style>

    <style name="option_item_switchbutton">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <!--Image-->
    <style name="UserFaceImageStyle">
        <item name="android:layout_width">40.0dip</item>
        <item name="android:layout_height">40.0dip</item>
        <item name="android:padding">1.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:contentDescription">face</item>
        <item name="android:src">@drawable/widget_dface</item>
    </style>

    <!--User Information-->
    <style name="user_information_ll">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/user_information_ll_background</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">@dimen/space_4</item>
    </style>

    <style name="user_information_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/space_2</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_11</item>
    </style>

    <style name="user_information_info" parent="@style/user_information_text">
        <item name="android:textSize">@dimen/text_size_12</item>
    </style>

    <style name="ques_bt_styles">
    <item name="android:layout_marginTop">10dp</item>
    <item name="android:layout_height">25dp</item>
    <item name="android:layout_width">48dp</item>
    <item name="android:layout_marginBottom">10dp</item>
    <item name="android:textSize">13sp</item>
    <item name="android:layout_marginLeft">4dp</item>
    <item name="android:layout_marginRight">4dp</item>
    <item name="android:background">@drawable/ques_bt_bg_selector</item>
    <item name="android:textColor">@color/ques_bt_text_color_light</item>
    </style>

    <!--quick option start-->
    <style name="quick_option_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">@dimen/space_16</item>
    </style>

    <style name="quick_option_img">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
    </style>

    <style name="quick_option_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/space_12</item>
        <item name="android:textColor">@color/quickoption_textcolor_selector</item>
        <item name="android:textSize">@dimen/text_size_12</item>
