        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/progress_loading</item>
        <item name="android:indeterminateDuration">1000</item>
        <item name="android:indeterminateOnly">true</item>
    </style>

    <style name="chat_progressbar">
        <item name="android:indeterminateDrawable">@drawable/progress_medium</item>
    </style>


    <style name="notebook_edit_menu">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">30dp</item>
    </style>

    <style name="notebook_edit_menu_item" parent="notebook_edit_menu">
